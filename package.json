{
  "name": "ServiceApp",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon server/index.js",
    "start-prod": "node index",
    "server": "pm2 start && pm2 logs",
    "webpack": "webpack -w --mode development",
    "dev": "npm-run-all -p server webpack",
    "build-webpack": "webpack -p",
    "build-node": "babel src/server -d build --copy-files",
    "build-static": "cp -r www build && cp -r node_modules build && cp -r views build",
    "build": "rm -Rf build && yarn build-webpack && yarn build-node && yarn build-static",
    "test": "echo \"Error: no test specified\"",
    "commit": "git add . && git commit",
    "push": "(git diff --quiet && git diff --staged --quiet || yarn commit) && git push origin"
  },
  "nodemonConfig": {
    "watch": [
      "src/server/"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.7.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "credential": "^2.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.2.0",
    "multer": "^1.4.4",
    "node-fpgrowth": "^1.2.1",
    "nodemon": "^2.0.15",
    "passport": "^0.5.0",
    "pm2": "^5.3.1",
    "pug": "^3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-material-ui-form-validator": "^3.0.0",
    "react-router-dom": "^6.0.2",
    "react-spinners": "^0.11.0",
    "winston": "^3.3.3",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  }
}
